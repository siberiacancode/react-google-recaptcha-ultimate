  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "shx rm -rf dist && rollup -c --bundleConfigAsCjs",
    "prepare": "husky install && npm run build",
    "type": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint . --ext ts --ext tsx --no-error-on-unmatched-pattern",
